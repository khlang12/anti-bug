        <ul class="tabs">
            <li class="tab-link current" data-tab="tab-1">function operation</li>
            <li class="tab-link" data-tab="tab-2">history</li>
        </ul>

        <div id="tab-1" class="tab-content current">
            <h3 class="tab-title">Deployed Contract</h3>
            <div class="contract__interaction"></div>
        </div>
        <div id="tab-2" class="tab-content">
            <h3 class="tab-title">History</h3>
            <div class="contract__interaction"></div>
        </div>
    </div>
    <script nonce="<%= nonce %>" src="<%= scriptUri %>"></script>
    <script nonce="<%= nonce %>" src="https://use.fontawesome.com/releases/v5.2.0/js/all.js"></script>
