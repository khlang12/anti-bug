{
  "name": "anti-ts",
  "displayName": "",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "antibug",
          "title": "AntiBug",
          "icon": "./images/logo.png"
        }
      ]
    },
    "views": {
      "antibug": [
        {
          "type": "webview",
          "name": "Interaction",
          "id": "antibug.primaryPanel.interaction"
        },
        {
          "type": "webview",
          "name": "Deploy & Function Interaction",
          "id": "antibug.deploy"
        },
        {
          "type": "webview",
          "name": "Security Analysis",
          "id": "antibug.security"
        },
        {
          "type": "webview",
          "name": "Test Code",
          "id": "antibug.testcode"
        }
      ]
    },
    "commands": [
      {
        "category": "antibug",
        "command": "sendAbiNames",
        "title": "Send ABI names"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/debug": "^4.1.10",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@ethereumjs/block": "^5.0.0",
    "@ethereumjs/blockchain": "^7.0.0",
    "@ethereumjs/common": "^4.0.0",
    "@ethereumjs/evm": "^2.0.0",
    "@ethereumjs/trie": "^6.0.0",
    "@ethereumjs/tx": "^5.0.0",
    "@ethereumjs/util": "^9.0.0",
    "@ethereumjs/vm": "^7.0.0",
    "@types/ejs": "^3.1.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "js": "^0.1.0",
    "markdown-it": "^13.0.2",
    "solc": "^0.8.22"
  }
}
